<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
>
    <bean id="people" class="com.softeem.autowire.People" autowire="constructor" >
        <property name="pName" value="李哲宇"></property>
    </bean>

    <bean id="dog2" class="com.softeem.autowire.Dog">
        <property name="name" value="柴犬"></property>
    </bean>
    <bean id="dog" class="com.softeem.autowire.Dog">
        <property name="name" value="哈士奇"></property>
    </bean>
</beans>